<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
    <value>|</value>
    <transformStatement>
        <appendcolsTransformation>
            <value>appendcols</value>
            <t_appendcols_overrideParameter>
                <value>override=</value>
                <booleanType>
                    <value>FALSE</value>
                </booleanType>
            </t_appendcols_overrideParameter>
            <t_appendcols_subsearchParameter>
                <t_appendcols_extendtimerangeParameter>
                    <value>extendtimerange=</value>
                    <booleanType>
                        <value>TRUE</value>
                    </booleanType>
                </t_appendcols_extendtimerangeParameter>
            </t_appendcols_subsearchParameter>
            <t_appendcols_subsearchParameter>
                <t_appendcols_maxtimeParameter>
                    <value>maxtime=</value>
                    <integerType>
                        <value>1</value>
                    </integerType>
                </t_appendcols_maxtimeParameter>
            </t_appendcols_subsearchParameter>
            <t_appendcols_subsearchParameter>
                <t_appendcols_maxoutParameter>
                    <value>maxout=</value>
                    <integerType>
                        <value>2</value>
                    </integerType>
                </t_appendcols_maxoutParameter>
            </t_appendcols_subsearchParameter>
            <t_appendcols_subsearchParameter>
                <t_appendcols_timeoutParameter>
                    <value>timeout=</value>
                    <integerType>
                        <value>3</value>
                    </integerType>
                </t_appendcols_timeoutParameter>
            </t_appendcols_subsearchParameter>
            <subsearchStatement>
                <value>[</value>
                <value>|</value>
                <transformStatement>
                    <addtotalsTransformation>
                        <value>addtotals</value>
                        <t_addtotals_labelParameter>
                            <value>label=</value>
                            <stringType>
                                <value>ddd</value>
                            </stringType>
                        </t_addtotals_labelParameter>
                        <fieldListType>
                            <fieldType>
                                <value>subdddd</value>
                            </fieldType>
                        </fieldListType>
                    </addtotalsTransformation>
                </transformStatement>
                <value>]</value>
            </subsearchStatement>
        </appendcolsTransformation>
    </transformStatement>
</root>
